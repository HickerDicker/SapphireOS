Windows Registry Editor Version 5.00

; disable server manager from opening at logon 

[HKEY_CURRENT_USER\Software\Microsoft\ServerManager]
"CheckedUnattendLaunchSetting"=dword:0000001
"DoNotOpenServerManagerAtLogon"=dword:0000001

[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\ServerManager]
"DoNotOpenServerManagerAtLogon"=dword:0000001
"DoNotPopWACConsoleAtSMLaunch"=dword:0000001